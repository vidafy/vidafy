@using Disco.Extensions.Abstractions 

@{
    ViewBag.PageTitle = "Print Slips";
    Layout = ViewBag.Layout;
    var SKU =  Context.Request.Query["SKU"];
    DateTime BegDate = new DateTime(1900, 1, 1);
    if (!DateTime.TryParse(Context.Request.Query["BegDate"], out BegDate))
    {
        BegDate = DateTime.Now.Date;
    }
    DateTime EndDate = DateTime.Today.AddDays(1);
    if (!DateTime.TryParse(Context.Request.Query["EndDate"], out EndDate))
    {
        EndDate = DateTime.Now.Date;
    }

    var Category = Context.Request.Query["Category"] ;
    var CatName = Context.Request.Query["CatName"] ;
    var Code = Context.Request.Query["Code"] ;
    var GetOrders = Context.Request.Query["GetOrders"];
    var PR = "";
    var PS = "";
    var PI = "";
    var PPS = "";
    var CSV = "";
}
<style>
    .input-group-btn {
        display: unset;
    }
</style>

@Html.Partial("Partials/DateInput", new Disco.Extensions.Abstractions.UI.DateInput
{
   Id = "DateRangePicker",
   Style = "min-width: 175px;",
   Class = "btn btn-default",
   Name = "BegDate",
   EndName = "EndDate",
   BegDate = BegDate,
   EndDate = EndDate
})